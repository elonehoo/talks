{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "installCommand": "pnpm install",
        "buildCommand": "pnpm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/2023/vue-devtools/(.*)",
      "dest": "/2023/vue-devtools/index.html"
    },
    {
      "src": "/2023/project-copy/(.*)",
      "dest": "/2023/project-copy/index.html"
    },
    {
      "src": "/2024/build-tauri/(.*)",
      "dest": "/2024/build-tauri/index.html"
    },
    {
      "src": "/",
      "status": 302,
      "headers": {
        "Location": "https://elonehoo.me/talks"
      }
    }
  ],
  "env": {
    "NODE_VERSION": "20"
  }
}
